<%= form_for @player do |f| %>
	<%= f.label "First Name" %>
	<%= f.text_field :fname %>

	<%= f.label "Last Name" %>
	<%= f.text_field :lname %>

	<%= f.label "Date of Birth" %>
	<%= f.text_field :dob %>

	<%= f.label :email %>
	<%= f.text_field :email %>

	<%= f.label :phone %>
	<%= f.text_field :phone %>

	<%= f.label :throws %>
	<%= f.password_field :throws %>

	<%= f.label :bats %>
	<%= f.password_field :bats %>

	<%= f.submit %>
<% end %>